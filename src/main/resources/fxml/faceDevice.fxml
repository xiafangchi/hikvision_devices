<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="465.0" prefWidth="929.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.ys.hikvision_device.controller.FaceDeviceController">

    <children>
        <TableView fx:id="faceDeviceTable" layoutX="12.0" layoutY="142.0" prefHeight="302.0" prefWidth="898.0">
            <columns>
                <TableColumn fx:id="cellNo" prefWidth="96.0" text="序号"/>
                <TableColumn fx:id="name" prefWidth="165.0" text="名称"/>
                <TableColumn fx:id="ip" prefWidth="239.0" text="IP"/>
                <TableColumn fx:id="direction" prefWidth="96.0" text="进出方向"/>
                <TableColumn fx:id="status" prefWidth="301.0" text="状态"/>
            </columns>
        </TableView>
        <Button fx:id="select" layoutX="49.0" layoutY="96.0" mnemonicParsing="false" onAction="#selectFaceDevice"
                prefHeight="23.0" prefWidth="71.0" text="查询" AnchorPane.leftAnchor="49.0"/>
        <Button fx:id="add" layoutX="223.0" layoutY="96.0" mnemonicParsing="false" onAction="#saveFaceDevice"
                prefHeight="23.0" prefWidth="71.0" text="新增" AnchorPane.leftAnchor="223.0"/>
        <Button fx:id="edit" layoutX="582.0" layoutY="96.0" mnemonicParsing="false" onAction="#editFaceDevice"
                prefHeight="23.0" prefWidth="71.0" text="修改" AnchorPane.leftAnchor="582.0"/>
        <Button fx:id="delete" layoutX="763.0" layoutY="96.0" mnemonicParsing="false" onAction="#deleteFaceDevice"
                prefHeight="23.0" prefWidth="71.0" text="删除" AnchorPane.leftAnchor="763.0"/>
        <Label layoutX="54.0" layoutY="52.0" text="名称"/>
        <TextField fx:id="selectDeviceName" layoutX="97.0" layoutY="47.0" prefHeight="30.0" prefWidth="252.0"/>
        <Label layoutX="545.0" layoutY="52.0" text="IP"/>
        <TextField fx:id="selectDeviceIp" layoutX="582.0" layoutY="47.0" prefHeight="30.0" prefWidth="252.0"/>
        <Button fx:id="deploy" layoutX="403.0" layoutY="96.0" mnemonicParsing="false" onAction="#deploy"
                prefHeight="23.0" prefWidth="71.0" text="布防"/>
    </children>

</AnchorPane>
