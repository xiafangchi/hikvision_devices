<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<AnchorPane prefHeight="273.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.ys.hikvision_device.controller.PersonnelEmployeeNoAndDepartmentController">
    <children>
        <Pane layoutX="18.0" layoutY="30.0" prefHeight="200.0" prefWidth="565.0">
            <children>
                <Label layoutX="66.0" layoutY="30.0" text="工号前缀"/>
                <TextField fx:id="addEmployeeNo" layoutX="133.0" layoutY="25.0" prefHeight="30.0" prefWidth="299.0"/>
                <Button fx:id="addDepartment" layoutX="136.0" layoutY="142.0" mnemonicParsing="false"
                        onAction="#saveDepartment" prefHeight="30.0" prefWidth="66.0" text="确认"/>
